<h1>Listing compilers</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Cmd line</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @compilers.each do |compiler| %>
  <tr>
    <td><%= compiler.title %></td>
    <td><%= compiler.cmd_line %></td>
    <td><%= link_to 'Show', compiler %></td>
    <td><%= link_to 'Edit', edit_compiler_path(compiler) %></td>
    <td><%= link_to 'Destroy', compiler, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Compiler', new_compiler_path %>
